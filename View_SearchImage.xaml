        <Button Name="btn_back" Content="뒤로 가기" HorizontalAlignment="Left" Height="52" Margin="40,35,0,0" VerticalAlignment="Top" Width="88"/>
        <Button Name="btn_search" Content="검색" HorizontalAlignment="Left" Height="52" Margin="672,35,0,0" VerticalAlignment="Top" Width="88"/>

    </Grid>
</Window>
